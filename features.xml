<NotepadPlus>
    <UserLang name="Features" ext="feature">
        <Settings>
            <Global caseIgnored="no" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">&quot;&lt;0&quot;&gt;0</Keywords>
            <Keywords name="Folder+"></Keywords>
            <Keywords name="Folder-"></Keywords>
            <Keywords name="Operators">:</Keywords>
            <Keywords name="Comment"> 1 1 2 2 0# 0|</Keywords>
            <Keywords name="Words1">Feature</Keywords>
            <Keywords name="Words2">Given When Then And But Examples </Keywords>
            <Keywords name="Words3">Scenario Outline Background</Keywords>
            <Keywords name="Words4">@</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="Verdana" fontStyle="0" fontSize="9" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="0000FF" bgColor="FFFFFF" fontName="Arial" fontStyle="0" fontSize="12" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="FF0000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="800000" bgColor="FFFFFF" fontName="Verdana" fontStyle="0" fontSize="14" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="008000" bgColor="FFFFFF" fontName="Consolas" fontStyle="2" fontSize="12" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="0000FF" bgColor="FFFFFF" fontName="Arial" fontStyle="1" fontSize="12" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="004000" bgColor="FFFFFF" fontName="Consolas" fontStyle="0" fontSize="10" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="804040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="804040" bgColor="FFFFFF" fontName="Consolas" fontStyle="0" fontSize="12" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="400080" bgColor="FFFFFF" fontName="Verdana" fontStyle="0" fontSize="9" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="0915B9" bgColor="FFFFFF" fontName="Verdana" fontStyle="0" fontSize="9" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
